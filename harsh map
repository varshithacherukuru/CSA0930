import java.util.*;
class Scratch {
    public static void main(String[] args) {
        Map<Integer,String> numbers = new HashMap<>() ;

        numbers.put(4,"abc");
        numbers.put(5,"def");
        numbers.put(7,"hkj");
        numbers.put(6,"okm");
        System.out.println("values added:" + numbers);
        numbers.remove(4);
        System.out.println("values removed after:" + numbers);
    }
}
