import java.util.Scanner;
class harshad {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int n = input.nextInt();
        int temp, sum = 0, rev, res;
        temp = n;
        while (n > 0) {
            rev = n % 10;
            sum = sum + rev;
            n /= 10;
        }
        if (temp%sum==0) {
            System.out.println("harshad");
        } else {
            System.out.println(" not harshad");
        }
    }
}
